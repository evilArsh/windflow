{
  "name": "windflow",
  "version": "0.7.5",
  "description": "Power your automated workflows with multiple AI providers",
  "main": "./out/main/index.js",
  "author": "windflow <arshdebian@163.com>",
  "type": "module",
  "private": "true",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "electron-builder install-app-deps",
    "format": "prettier --write .",
    "prepare": "husky",
    "commitlint": "commitlint -e -V",
    "lint-staged": "lint-staged",
    "lint": "npm run typecheck:node && npm run typecheck:web && eslint ./src/renderer/src",
    "lint-fix": "eslint ./src/renderer/src --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "node ./scripts/dev.ts",
    "build": "tsx ./scripts/build.ts build",
    "build:unpack": "tsx ./scripts/build.ts build:unpack",
    "build:win": "tsx ./scripts/build.ts build:win",
    "build:mac": "tsx ./scripts/build.ts build:mac",
    "build:linux": "tsx ./scripts/build.ts build:linux",
    "test:web": "vitest --project unit-web",
    "test:node": "vitest --project unit-node"
  },
